// Testing LP


// Feasible solution (exactly one solution: x1=1, x2=1, and x3=1)
LP [
    ([1, 1, 1], "<=", 3),
    ([1, 1, 0], ">=", 2),
    ([0, 0, 1], ">=" , 1)
];

// Infeasible
LP [
    ([1, 1, 1], "<=", 3),
    ([1, 1, 0], ">=", 2),
    ([0, 0, 1], ">" , 1)
];

